{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "ter-indent": 4,
        "import-name": false,
        "no-duplicate-variable": true,
        "quotemark": [true, "double"],
        "brace-style": [
            true,
            "allman",
            {
                "allowSingleLine": true
            }
        ]
    },
    "rulesDirectory": []
}